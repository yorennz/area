{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/area-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/area-api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/area-api",
  "projects": {
    "area-api": {
      "type": "application",
      "root": "apps/area-api",
      "entryFile": "main",
      "sourceRoot": "apps/area-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/area-api/tsconfig.app.json"
      }
    },
    "area-trigger": {
      "type": "application",
      "root": "apps/area-trigger",
      "entryFile": "main",
      "sourceRoot": "apps/area-trigger/src",
      "compilerOptions": {
        "tsConfigPath": "apps/area-trigger/tsconfig.app.json"
      }
    }
  }
}
{
    "version": 2,
    "builds": [
        {
            "src": "apps/area-api/src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "apps/area-api/src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
            ]
        }
    ]
}
